# Example environment file for Claude Docker Swarm
# Copy this to .env and fill in your actual values

# REQUIRED: Anthropic Claude API Key
# Get from: https://console.anthropic.com/
CLAUDE_API_KEY=your_anthropic_api_key_here

# Database Configuration
NEO4J_USER=neo4j
NEO4J_PASSWORD=swarm_password
NEO4J_HOST=neo4j
NEO4J_PORT=7687

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=

# Agent Configuration
ARCHITECT_PORT=3001
FRONTEND_PORT=3002
BACKEND_PORT=3003
DEVOPS_PORT=3004
QA_PORT=3005
DOCS_PORT=3006
REVIEWER_PORT=3007
DASHBOARD_PORT=3000

# Optional: Additional API Keys
OPENAI_API_KEY=your_openai_api_key_here
GITHUB_TOKEN=your_github_token_here

# Reviewer Agent Configuration
CONTEXT7_API_KEY=your_context7_api_key_here
CONTEXT7_WORKSPACE=your_workspace_id_here
BRAVE_API_KEY=your_brave_search_api_key_here
SONARQUBE_URL=https://your-sonarqube-instance.com
SONARQUBE_TOKEN=your_sonarqube_token_here

# Environment
NODE_ENV=production
LOG_LEVEL=info

# Debug Configuration
DEBUG_MODE=false

# Set DEBUG_MODE=true to prevent Claude API calls and simulate task processing
# This is useful for testing the workflow without incurring API costs

# Security (change in production)
JWT_SECRET=your_jwt_secret_here
SESSION_SECRET=your_session_secret_here
